{
  "name": "opfs-viewer",
  "description": "Original private file system viewer extension for Chrome",
  "version": "1.0.0",
  "manifest_version": 3,
  "devtools_page": "devtools.html",
  "background": {
    "service_worker": "serviceWorker.js"
  },
  "content_scripts": [
    {
      "matches": ["<all_urls>"],
      "js": ["contentScript.js"]
    }
  ],
  "icons": {
    "16": "/icons/16x.png",
    "32": "/icons/32x.png",
    "48": "/icons/48x.png",
    "128": "/icons/128x.png"
  }
}